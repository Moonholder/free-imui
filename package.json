    "@rollup/plugin-node-resolve": "^13.3.0",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-loader": "^8.2.5",
    "less": "^3.0.4",
